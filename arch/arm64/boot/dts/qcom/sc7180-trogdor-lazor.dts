/dts-v1/;

#include "sc7180.dtsi"

ap_ec_spi: &spi6 {};
ap_h1_spi: &spi0 {};

#include "sc7180-trogdor.dtsi"

/ {
	model = "Google Lazor (rev0+)";
	compatible = "google,lazor", "qcom,sc7180";

	panel: panel {
		compatible = "boe,nv133fhmn62";
		power-supply = <&pp3300_dx_edp>;
		backlight = <&backlight>;
		no-hpd; /* TODO: Remove? */

		ports {
			port {
				panel_in_edp: endpoint {
					remote-endpoint = <&sn65dsi86_out>;
				};
			};
		};
	};
};

